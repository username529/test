#include <iostream>
using namespace std;

int main() {
    int число1, число2;
    
число1 = 10; 
число2 = 20;

    if (число1 > число2) {
        cout << "Первое число больше второго.";
    } else if (число2 > число1) {
        cout << "Второе число больше первого.";
    } else {
        cout << "Числа равны.";
    }
    return 0;
}
